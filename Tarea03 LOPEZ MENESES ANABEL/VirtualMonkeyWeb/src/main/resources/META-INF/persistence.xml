    <class>rysi.tesis.virtualmonkeyweb.Usuario</class>
    <class>rysi.tesis.virtualmonkeyweb.Gradoacademico</class>
    <class>rysi.tesis.virtualmonkeyweb.Tesis</class>
    <class>rysi.tesis.virtualmonkeyweb.Disciplinaestudio</class>
    <class>rysi.tesis.virtualmonkeyweb.Materialextra</class>
    <class>rysi.tesis.virtualmonkeyweb.Especieprimate</class>
    <class>rysi.tesis.virtualmonkeyweb.Consulta</class>
