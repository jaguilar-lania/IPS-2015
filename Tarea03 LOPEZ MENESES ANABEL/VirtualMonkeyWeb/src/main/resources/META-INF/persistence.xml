    <class>rysi.tesis.virtualmonkeyweb.Personaltesis</class>
    <class>rysi.tesis.virtualmonkeyweb.Sesion</class>
    <class>rysi.tesis.virtualmonkeyweb.Rol</class>
    <class>rysi.tesis.virtualmonkeyweb.Retroalimentacion</class>
    <class>rysi.tesis.virtualmonkeyweb.Condicionsitio</class>
    <class>rysi.tesis.virtualmonkeyweb.Pais</class>
    <class>rysi.tesis.virtualmonkeyweb.Subdisciplina</class>
    <class>rysi.tesis.virtualmonkeyweb.Estado</class>
    <class>rysi.tesis.virtualmonkeyweb.Autor</class>
    <class>rysi.tesis.virtualmonkeyweb.Directortesis</class>
    <class>rysi.tesis.virtualmonkeyweb.Institucion</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/VirtualMonkey"/>
      <property name="javax.persistence.jdbc.user" value="administrador"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="admin"/>
    </properties>
  </persistence-unit>
</persistence>
